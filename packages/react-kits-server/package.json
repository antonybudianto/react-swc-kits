{
  "name": "react-kits-server",
  "version": "5.0.0",
  "description": "Express server for react-kits",
  "source": "src/index.js",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "microbundle --jsx=React.createElement",
    "watch": "microbundle watch --jsx=React.createElement --compress=false",
    "test": "jest --env=node --coverage",
    "test:dev": "jest --env=node --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antonybudianto/react-kits-server.git"
  },
  "author": "Antony",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antonybudianto/react-kits-server/issues"
  },
  "homepage": "https://github.com/antonybudianto/react-kits-server#readme",
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules",
      "../react-kits-cli/template/node_modules"
    ]
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-helmet-async": "1.1.2",
    "react-kits": "*",
    "react-router-dom": "*"
  },
  "dependencies": {
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "serialize-javascript": "^1.5.0"
  },
  "devDependencies": {
    "microbundle": "0.6.0"
  }
}
