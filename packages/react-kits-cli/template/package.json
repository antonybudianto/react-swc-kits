{
  "name": "~~name~~",
  "version": "6.0.0",
  "description": "react ssr starter",
  "private": true,
  "scripts": {
    "dev": "bnr start",
    "clean": "react-kits clean",
    "lint": "react-kits lint",
    "dll": "react-kits build-dll",
    "test": "jest",
    "build": "npm run build:prod",
    "build:dev": "bnr build:dev",
    "build:staging": "bnr build:staging",
    "build:prod": "bnr build:prod",
    "serve": "bnr serve",
    "servelocal": "bnr servelocal",
    "prettier": "prettier --write \"src/**/*.js\""
  },
  "betterScripts": {
    "start": {
      "command": "react-kits start",
      "env": {
        "NODE_ENV": "development",
        "APP_ENV": "development",
        "APP_ASSET_PATH": "/myapp/"
      }
    },
    "serve": {
      "command": "node dist/bundle.js",
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "production"
      }
    },
    "servelocal": {
      "command": "node --preserve-symlinks dist/bundle.js",
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "production"
      }
    },
    "build:dev": {
      "command": "react-kits build",
      "env": {
        "NODE_ENV": "development",
        "APP_ENV": "development"
      }
    },
    "build:staging": {
      "command": "react-kits build",
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "staging",
        "APP_ASSET_PATH": "/myapp/"
      }
    },
    "build:prod": {
      "command": "react-kits build",
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "production",
        "APP_ASSET_PATH": "/myapp/"
      }
    }
  },
  "author": "User",
  "browserslist": [
    "> 1% in ID",
    "last 2 versions",
    "Firefox ESR",
    "iOS >= 9",
    "not ie < 11"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "axios": "0.18.0",
    "basscss": "8.0.4",
    "better-npm-run": "^0.1.1",
    "classnames": "^2.2.6",
    "dotenv": "6.2.0",
    "lodash": "4.17.11",
    "lodash-es": "4.17.11",
    "npm-run-all": "4.1.5",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "raf": "3.4.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-helmet-async": "1.1.2",
    "react-hot-loader": "^4.8.0",
    "react-kits-server": "^6.0.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "react-ua": "0.2.0",
    "ua-parser-js": "0.7.18",
    "graphql": "16.3.0",
    "urql": "2.2.0",
    "@urql/core": "2.4.4",
    "isomorphic-unfetch": "3.1.0"
  },
  "devDependencies": {
    "http-proxy-middleware": "^0.19.1",
    "jest": "27.5.1",
    "@swc/jest": "0.2.20",
    "@swc/core": "1.2.89",
    "prettier": "1.15.3",
    "react-kits": "^6.0.0",
    "webpack-hot-middleware": "^2.24.3"
  }
}
